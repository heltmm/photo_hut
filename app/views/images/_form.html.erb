<div class="field">

</div>


<%= render "layouts/errors", :object => @image %>

<%= form_for [@image] do |f| %>
  <%= f.label :photo, class: 'col-sm-2 control-label'  %>
  <div class="col-sm-6">
      <%= f.file_field :photo %>
  </div>
  <%= f.label :description %><br>
  <%= f.text_field :description %><br>
  <%= f.hidden_field(:user_id, :value => current_user.id) %>
  <%= f.label :tag %><br>
  
  <%= f.submit %>
<% end %>
